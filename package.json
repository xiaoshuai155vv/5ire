    "tsx": "^4.19.2",
    "typeid-js": "^0.3.0",
    "typescript": "^5.1.3",
    "update-electron-app": "^3.0.0",
    "url-loader": "^4.1.1",
    "valid-url": "^1.0.9",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
